{
  "name": "serverless-graphql-apollo",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:serverless/serverless-graphql-apollo.git"
  },
  "scripts": {
    "start-client-local": "cd ../app-client/ && npm run dev",
    "start-server-lambda:offline": "env SLS_DEBUG=true GRAPHQL_ENDPOINT=/graphql CLIENT_URL=http://localhost:3000 serverless -s dev offline start",
    "deploy-server-lambda-prod": "serverless --stage=production deploy",
    "lint": "eslint . --cache"
  },
  "dependencies": {
    "apollo-server-lambda": "1.1.2",
    "apollo-server-module-graphiql": "^1.1.2",
    "babel-core": "^6.17.0",
    "babel-polyfill": "^6.16.0",
    "babel-runtime": "^6.25.0",
    "graphql": "^0.10.5",
    "graphql-tools": "^1.0.0",
    "mongodb": "^2.2.31",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "serverless-delete-loggroups": "^0.3.1",
    "serverless-plugin-warmup": "^3.2.0-rc.1"
  },
  "devDependencies": {
    "aws-sdk": "^2.4.8",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "body-parser": "^1.15.2",
    "cors": "2.8.4",
    "env-cmd": "^5.0.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-flowtype": "^2.11.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.2.0",
    "imports-loader": "^0.7.1",
    "serverless-offline": "^3.15.3",
    "serverless-webpack": "^2.2.0",
    "webpack": "^2.2.0",
    "webpack-node-externals": "^1.6.0"
  }
}
